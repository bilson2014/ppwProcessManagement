  <process id="vacation" name="员工请假流程" isExecutable="true">
    <documentation>员工请假流程</documentation>
    <startEvent id="startevent" name="请假申请提交" activiti:initiator="applyUserId">
      <documentation>员工请假流程</documentation>
    </startEvent>
    <userTask id="deptLeaderAudit" name="部门领导审批" activiti:candidateGroups="deptLeader">
      <documentation>部门领导审批</documentation>
    </userTask>
    <sequenceFlow id="sid-8CCE1759-F480-48B6-A0AB-A0F30F4DACDB" sourceRef="startevent" targetRef="deptLeaderAudit"></sequenceFlow>
